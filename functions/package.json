    "better-module-alias": "^1.1.0",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.11.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
